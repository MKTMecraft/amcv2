    jQuery(document).ready(function () {
        locationIds = @Html.Raw(ViewBag.LocationIds);
        locationNames = @Html.Raw(ViewBag.LocationName);
        wineNames = @Html.Raw(ViewBag.WineName);
        const queryString = window.location.search;
            const urlParams = new URLSearchParams(queryString);
            const page = urlParams.get('page');
            if (page == "wine") {
                $("#location").removeClass("active");
                $("#wine").addClass("active");
                $("#Home-withicon").removeClass("active");
                $("#Profile-withicon").addClass("active");
        }
        autocomplete(document.getElementById("LocationId"), locationIds);
        autocomplete(document.getElementById("LocationName"), locationNames);
        autocomplete(document.getElementById("WineName"), wineNames);
        TableDatatablesColreorder.init();
        TableDatatablesColreorder2.init();
    });

    function OpenLocationFilters() {
        $("#by_location").modal("show");
    }

    function locationSearch() {
        $("#by_location").modal("hide");
         TableDatatablesColreorder.init();
    }

    function locationReset() {
        $("#LocationId").val('');
        $("#LocationName").val('');
        $("#StateId").val('');
        $("#ServiceTypeId").val('');
        $("#BarTypeId").val('');
        $("#by_location").modal("hide");
        TableDatatablesColreorder.init();
    }

    function OpenWineFilters() {
