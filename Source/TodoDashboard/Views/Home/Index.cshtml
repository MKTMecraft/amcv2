<div class="modal animated fadeIn" id="by_location" tabindex="-1" role="dialog">
    <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content">
            <form>
                <div class="modal-header">
                    <h6 class="title" id="defaultModalLabel">Advance Search</h6>
                </div>
                <div class="modal-body">
                    <div>
                        <div class="row clearfix">
                            <div class="col-md-4 col-sm-12">
                                <label>Id</label>
                                <div class="form-group autocomplete">
                                    <input type="text" id="LocationId" class="form-control" placeholder="Enter Id">
                                </div>
                            </div>
                            <div class="col-md-4 col-sm-12">
                                <label>Location</label>
                                <div class="form-group autocomplete">
                                    <input type="text" id="LocationName" class="form-control" placeholder="Enter Location">
                                </div>
                            </div>
                            <div class="col-md-4 col-sm-12">
                                <label>State</label>
                                <div class="form-group">
                                    @Html.DropDownList("StateId", (IEnumerable<SelectListItem>)ViewBag.States, "Select State", new { @Id = "StateId", @class = "form-control show-tick ms select2" })
                                </div>
                            </div>
                        </div>
                        <div class="row clearfix">
                            <div class="col-md-4 col-sm-12">
                                <label>Service Type</label>
                                <div class="form-group">
                                    @Html.DropDownList("ServiceTypeId", (IEnumerable<SelectListItem>)ViewBag.ServiceTypes, "Select Service Type", new { @Id = "ServiceTypeId", @class = "form-control show-tick ms select2" })
                                </div>
                            </div>
                            <div class="col-md-4 col-sm-12">
                                <label>Bar Type</label>
                                <div class="form-group">
                                    @Html.DropDownList("BarTypeId", (IEnumerable<SelectListItem>)ViewBag.BarTypes, "Select Bar Type", new { @Id = "BarTypeId", @class = "form-control show-tick ms select2" })
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="modal-footer">
                    <!--<button type="button" class="btn btn-primary">Add</button>-->
                    <button type="button" class="btn btn-primary saveBtn" onclick="locationSearch();">Search</button>
                    <button class="btn btn-primary loading" type="button" disabled="" style="        display: none;">
                        <span class="spinner-border spinner-border-sm" role="status" aria-hidden="true"></span>
                        Searching...
                    </button>
                    <button type="reset" class="btn btn-secondary closeBtn" onclick="locationReset();">Reset</button>
                </div>
            </form>
        </div>
    </div>
</div>


<div class="modal animated fadeIn" id="by_wine" tabindex="-1" role="dialog">
